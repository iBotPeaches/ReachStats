ion greenEmblem(){document.getElementById("1_green").click();if(document.getElementById("spartan_enable").checked==false)document["sig2"].src="{$this->settings['_original_base_url']}/ccs_files/pack2_1.png";else document["sig2"].src="{$this->settings['_original_base_url']}/ccs_files/pack2.png"}function checkEmblem(num,id){if(id==1){if(num==1){document["sig2"].src="{$this->settings['_original_base_url']}/ccs_files/pack2.png";document.getElementById("spartan_enable").checked=true}else{document["sig2"].src="{$this->settings['_original_base_url']}/ccs_files/pack2_1.png";document.getElementById("spartan_enable").checked=false}}else{document.getElementById("spartan_enable").checked=false}}function changeMessage(){document.getElementById("funnyMessage").innerHTML='<div class="reg_msg reg_error">Test this is an error</div>'}</script>
